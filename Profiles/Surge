[Proxy Group]
PROXY = select, 星国, 日本, 美国, 香港, no-alert=0, hidden=0, include-all-proxies=0
星国 = url-test, policy-path=订阅链接, update-interval=0, no-alert=0, hidden=1, include-all-proxies=0, policy-regex-filter=新加坡|SG
日本 = url-test, policy-path=订阅链接, update-interval=0, no-alert=0, hidden=1, include-all-proxies=0, policy-regex-filter=日本|JP
美国 = url-test, policy-path=订阅链接, update-interval=0, no-alert=0, hidden=1, include-all-proxies=0, policy-regex-filter=美国|US
